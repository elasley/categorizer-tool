# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Configuration
REACT_APP_OPENAI_MODEL=gpt-4o-mini
REACT_APP_OPENAI_MAX_TOKENS=200
REACT_APP_OPENAI_TEMPERATURE=0.1

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_API_BASE_URL=http://localhost:3000

# Feature Flags
REACT_APP_ENABLE_AI_CATEGORIZATION=true
REACT_APP_ENABLE_BATCH_PROCESSING=true
REACT_APP_ENABLE_COST_ESTIMATION=true

# Processing Configuration
REACT_APP_BATCH_SIZE=10
REACT_APP_RATE_LIMIT_DELAY=1000
REACT_APP_MAX_FILE_SIZE=52428800

# Debug Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info